#include <stdio.h>
int main() {
    int a[10],n,i,*p,sum=0;
    p=a;
    printf("enter no.of elements:");
    scanf("%d",&n);
    printf("enter array elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",p+i);
    }
    for(i=0;i<n;i++)
    {
        sum=sum+p[i];
    }
    printf("sum=%d\n",sum);
    return 0;
}
